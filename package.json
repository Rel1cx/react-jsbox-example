{
  "name": "react-jsbox-example",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "dev": "run-p bundle watch",
    "watch": "jsbox watch dist",
    "bundle": "parcel watch src/main.js --no-source-maps --no-hmr",
    "clean": "rimraf .cache",
    "build": "npm run clean && parcel build src/main.js --no-source-maps",
    "build:box": "jsbox build dist",
    "lint": "eslint --fix src/**/*.js",
    "format": "prettier --ignore-path .gitignore --single-quote --write src/**/*.{js,md}",
    "postinstall": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint && git add ."
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "iOS 12"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "iOS 12"
        }
      ],
      "@babel/preset-react"
    ]
  },
  "url-loader": {
    "exts": [
      "jpg",
      "jpeg",
      "png",
      "gif"
    ],
    "limit": 10240
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-plugin-jsbox": "^0.1.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "jsbox-cli": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "parcel-plugin-url-loader": "^1.3.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "htm": "^3.0.4",
    "immer": "^7.0.9",
    "invert-color": "^2.0.0",
    "lodash-es": "^4.17.15",
    "pupa": "github:nicify/pupa#master",
    "react": "^16.13.1",
    "react-jsbox": "^1.2.7",
    "swr": "^0.3.5",
    "use-immer": "^0.4.1",
    "use-simple-store": "^1.3.1"
  },
  "staticFiles": {
    "staticPath": [
      "src"
    ],
    "watcherGlob": "^**"
  }
}
